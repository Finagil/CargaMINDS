                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT     WEB_Facturas.Factura, MAX(WEB_Facturas.ImporteFactura) AS ImporteFactura, SUM(ISNULL(WEB_Pagos.Importe, 0)) AS ImportePago
FROM         WEB_Facturas LEFT OUTER JOIN
                      WEB_Pagos ON WEB_Facturas.Factura = WEB_Pagos.Factura
